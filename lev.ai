                rgba(255, 136, 0, 0.3) 25%,
                rgba(255, 255, 0, 0.3) 50%,
                rgba(136, 255, 0, 0.3) 75%,
                rgba(0, 255, 136, 0.3) 100%);
            border-radius: 2px;
        }

        .timeline-progress {
            position: absolute;
            top: 0;
            left: 0;
            height: 100%;
            width: 0%;
            background: linear-gradient(to right, #ff4444, #ff8800, #ffff00, #88ff00, #00ff88);
            border-radius: 2px;
            transition: width 3s ease;
        }

        .timeline-items {
            display: grid;
            grid-template-columns: repeat(8, 1fr);
            gap: 20px;
            position: relative;
            z-index: 10;
        }

        .timeline-item {
            text-align: center;
            position: relative;
        }

        .timeline-dot {
            width: 20px;
            height: 20px;
            border-radius: 50%;
            margin: 0 auto 16px;
            background: rgba(255, 255, 255, 0.2);
            border: 3px solid rgba(255, 255, 255, 0.1);
            transition: all 0.5s ease;
            position: relative;
        }

        .timeline-dot.completed {
            background: #00ff88;
            border-color: #00ff88;
            box-shadow: 0 0 20px rgba(0, 255, 136, 0.4);
        }

        .timeline-dot.current {
            background: #ffff00;
            border-color: #ffff00;
            box-shadow: 0 0 20px rgba(255, 255, 0, 0.4);
            animation: pulse 2s infinite;
        }

        @keyframes pulse {
            0%, 100% { transform: scale(1); }
            50% { transform: scale(1.2); }
        }

        .timeline-year {
            font-size: 16px;
            font-weight: 700;
            color: #ffffff;
            margin-bottom: 8px;
        }

        .timeline-event {
            font-size: 12px;
            color: rgba(255, 255, 255, 0.6);
            line-height: 1.3;
        }

        /* Visionaries */
        .visionary-grid {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 60px;
            max-width: 1200px;
            margin: 0 auto;
        }

        .visionary-card {
            background: rgba(255, 255, 255, 0.02);
            border: 1px solid rgba(255, 255, 255, 0.08);
            border-radius: 24px;
            padding: 48px;
            position: relative;
            overflow: hidden;
        }

        .visionary-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 1px;
            background: linear-gradient(90deg, transparent, rgba(0, 255, 136, 0.3), transparent);
        }

        .visionary-header {
            display: flex;
            align-items: center;
            gap: 24px;
            margin-bottom: 32px;
        }

        .visionary-avatar {
            width: 80px;
            height: 80px;
            border-radius: 50%;
            background: linear-gradient(135deg, #00ff88, #0088ff);
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 32px;
            font-weight: 900;
            color: #000;
        }

        .visionary-info h3 {
            font-size: 24px;
            font-weight: 700;
            margin-bottom: 8px;
        }

        .visionary-title {
            font-size: 14px;
            color: #00ff88;
            font-weight: 600;
            margin-bottom: 4px;
        }

        .visionary-company {
            font-size: 12px;
            color: rgba(255, 255, 255, 0.5);
        }

        .visionary-quote {
            font-size: 18px;
            line-height: 1.6;
            color: rgba(255, 255, 255, 0.9);
            font-style: italic;
            margin-bottom: 32px;
        }

        .visionary-achievements {
            background: rgba(0, 255, 136, 0.05);
            border: 1px solid rgba(0, 255, 136, 0.1);
            border-radius: 16px;
            padding: 24px;
        }

        .achievements-title {
            font-size: 14px;
            font-weight: 600;
            color: #00ff88;
            margin-bottom: 16px;
        }

        .achievement-item {
            font-size: 14px;
            color: rgba(255, 255, 255, 0.7);
            margin-bottom: 8px;
            padding-left: 16px;
            position: relative;
        }

        .achievement-item::before {
            content: '→';
            position: absolute;
            left: 0;
            color: #00ff88;
        }

        /* Action Cards */
        .action-grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 40px;
            margin-top: 60px;
        }

        .action-card {
            background: rgba(255, 255, 255, 0.02);
            border: 1px solid rgba(255, 255, 255, 0.08);
            border-radius: 20px;
            padding: 40px;
            text-align: center;
            transition: all 0.4s ease;
            position: relative;
            overflow: hidden;
        }

        .action-card:hover {
            background: rgba(255, 255, 255, 0.04);
            border-color: rgba(0, 255, 136, 0.2);
            transform: translateY(-8px);
        }

        .action-icon {
            width: 100px;
            height: 100px;
            border-radius: 50%;
            background: linear-gradient(135deg, rgba(0, 255, 136, 0.2), rgba(0, 136, 255, 0.2));
            display: flex;
            align-items: center;
            justify-content: center;
            margin: 0 auto 24px;
            border: 2px solid rgba(0, 255, 136, 0.3);
        }

        .action-title {
            font-size: 24px;
            font-weight: 700;
            margin-bottom: 16px;
            font-family: 'Space Grotesk', sans-serif;
        }

        .action-description {
            font-size: 16px;
            color: rgba(255, 255, 255, 0.7);
            line-height: 1.6;
            margin-bottom: 24px;
        }

        .action-details {
            background: rgba(0, 0, 0, 0.3);
            border-radius: 12px;
            padding: 20px;
            text-align: left;
            margin-bottom: 20px;
        }

        .action-details h4 {
            font-size: 14px;
            font-weight: 600;
            color: #00ff88;
            margin-bottom: 12px;
        }

        .action-details ul {
            list-style: none;
            space-y: 6px;
        }

        .action-details li {
            font-size: 13px;
            color: rgba(255, 255, 255, 0.7);
            margin-bottom: 6px;
            padding-left: 16px;
            position: relative;
        }

        .action-details li::before {
            content: '✓';
            position: absolute;
            left: 0;
            color: #00ff88;
            font-weight: bold;
        }

        .impact-metric {
            background: linear-gradient(135deg, rgba(0, 255, 136, 0.1), rgba(0, 136, 255, 0.1));
            border: 1px solid rgba(0, 255, 136, 0.2);
            border-radius: 12px;
            padding: 16px;
            text-align: center;
        }

        .impact-number {
            font-size: 20px;
            font-weight: 800;
            color: #00ff88;
        }

        .impact-label {
            font-size: 12px;
            color: rgba(255, 255, 255, 0.6);
            margin-top: 4px;
        }

        /* Research Progress */
        .research-grid {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 40px;
            margin-top: 60px;
        }

        .research-category {
            background: rgba(255, 255, 255, 0.02);
            border: 1px solid rgba(255, 255, 255, 0.08);
            border-radius: 20px;
            padding: 40px;
            position: relative;
        }

        .category-header {
            display: flex;
            align-items: center;
            gap: 16px;
            margin-bottom: 24px;
        }

        .category-icon {
            width: 60px;
            height: 60px;
            border-radius: 12px;
            background: linear-gradient(135deg, rgba(0, 255, 136, 0.2), rgba(0, 136, 255, 0.2));
            display: flex;
            align-items: center;
            justify-content: center;
            border: 1px solid rgba(0, 255, 136, 0.3);
        }

        .category-title {
            font-size: 20px;
            font-weight: 700;
            font-family: 'Space Grotesk', sans-serif;
        }

        .breakthrough-item {
            background: rgba(0, 0, 0, 0.2);
            border-radius: 12px;
            padding: 20px;
            margin-bottom: 16px;
            border-left: 3px solid #00ff88;
        }

        .breakthrough-title {
            font-size: 16px;
            font-weight: 600;
            margin-bottom: 8px;
        }

        .breakthrough-description {
            font-size: 14px;
            color: rgba(255, 255, 255, 0.7);
            line-height: 1.5;
            margin-bottom: 12px;
        }

        .breakthrough-status {
            font-size: 12px;
            font-weight: 600;
            color: #00ff88;
            background: rgba(0, 255, 136, 0.1);
            padding: 4px 12px;
            border-radius: 20px;
            display: inline-block;
        }

        /* Resources */
        .resource-categories {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 60px;
            margin-top: 60px;
        }

        .resource-category {
            background: rgba(255, 255, 255, 0.02);
            border: 1px solid rgba(255, 255, 255, 0.08);
            border-radius: 20px;
            padding: 40px;
        }

        .resource-header {
            text-align: center;
            margin-bottom: 32px;
        }

        .resource-header h3 {
            font-size: 28px;
            font-weight: 700;
            font-family: 'Space Grotesk', sans-serif;
            margin-bottom: 12px;
            background: linear-gradient(135deg, #00ff88, #0088ff);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        .resource-list {
            space-y: 20px;
        }

        .resource-item {
            background: rgba(0, 0, 0, 0.2);
            border-radius: 16px;
            padding: 24px;
            margin-bottom: 20px;
            transition: all 0.3s ease;
            text-decoration: none;
            color: inherit;
            display: block;
            border: 1px solid rgba(255, 255, 255, 0.05);
        }

        .resource-item:hover {
            background: rgba(0, 0, 0, 0.3);
            border-color: rgba(0, 255, 136, 0.2);
            transform: translateY(-2px);
            text-decoration: none;
            color: inherit;
        }

        .resource-meta {
            display: flex;
            align-items: center;
            gap: 16px;
            margin-bottom: 12px;
        }

        .resource-avatar {
            width: 50px;
            height: 50px;
            border-radius: 8px;
            background: linear-gradient(135deg, #00ff88, #0088ff);
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 20px;
            color: #000;
            font-weight: bold;
        }

        .resource-title {
            font-size: 18px;
            font-weight: 600;
            margin-bottom: 4px;
        }

        .resource-author {
            font-size: 14px;
            color: rgba(255, 255, 255, 0.6);
        }

        .resource-description {
            font-size: 14px;
            color: rgba(255, 255, 255, 0.7);
            line-height: 1.5;
        }

        /* Call to Action */
        .cta-section {
            background: linear-gradient(135deg, rgba(0, 255, 136, 0.05), rgba(0, 136, 255, 0.05));
            border: 1px solid rgba(0, 255, 136, 0.1);
            border-radius: 24px;
            padding: 60px;
            text-align: center;
            margin-top: 80px;
        }

        .cta-title {
            font-size: 40px;
            font-weight: 700;
            font-family: 'Space Grotesk', sans-serif;
            margin-bottom: 20px;
            background: linear-gradient(135deg, #00ff88, #0088ff);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        .cta-buttons {
            display: flex;
            gap: 24px;
            justify-content: center;
            margin-top: 40px;
        }

        .btn-primary {
            background: linear-gradient(135deg, #00ff88, #0088ff);
            color: #000;
            padding: 16px 32px;
            border-radius: 12px;
            font-weight: 600;
            text-decoration: none;
            transition: all 0.3s ease;
            border: none;
            cursor: pointer;
        }

        .btn-primary:hover {
            transform: translateY(-2px);
            box-shadow: 0 10px 30px rgba(0, 255, 136, 0.3);
            text-decoration: none;
            color: #000;
        }

        .btn-secondary {
            background: transparent;
            color: #00ff88;
            padding: 16px 32px;
            border-radius: 12px;
            font-weight: 600;
            text-decoration: none;
            transition: all 0.3s ease;
            border: 2px solid #00ff88;
        }

        .btn-secondary:hover {
            background: #00ff88;
            color: #000;
            transform: translateY(-2px);
            text-decoration: none;
        }

        /* Responsive */
        @media (max-width: 1200px) {
            .nav-menu {
                gap: 24px;
            }
            
            .progress-hero {
                grid-template-columns: 1fr;
                gap: 40px;
                text-align: center;
            }
            
            .lev-circle {
                width: 250px;
                height: 250px;
            }
        }

        @media (max-width: 768px) {
            .container {
                padding: 0 20px;
            }
            
            .content-section {
                padding: 100px 20px 40px;
            }
            
            .section-title {
                font-size: 40px;
            }
            
            .visionary-grid,
            .action-grid,
            .research-grid,
            .resource-categories {
                grid-template-columns: 1fr;
            }
            
            .timeline-items {
                grid-template-columns: repeat(4, 1fr);
                gap: 10px;
            }
            
            .nav-menu {
                gap: 16px;
            }
            
            .nav-item {
                font-size: 13px;
            }
            
            .cta-buttons {
                flex-direction: column;
                align-items: center;
            }
        }
    </style>
</head>
