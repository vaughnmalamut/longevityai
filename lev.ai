        // Timeline data
        const timelineData = [
            { year: '2010', event: 'Human Genome Project Completion', status: 'completed' },
            { year: '2012', event: 'CRISPR-Cas9 Development', status: 'completed' },
            { year: '2016', event: 'AlphaFold Protein Folding', status: 'completed' },
            { year: '2020', event: 'COVID-19 Vaccine Speed', status: 'completed' },
            { year: '2023', event: 'AI Drug Discovery', status: 'completed' },
            { year: '2025', event: 'Current Progress Point', status: 'current' },
            { year: '2035', event: 'LEV Breakthrough', status: 'future' },
            { year: '2045', event: 'Widespread Adoption', status: 'future' }
        ];

        let currentSection = 'progress';

        // Initialize everything
        document.addEventListener('DOMContentLoaded', function() {
            if (typeof lucide !== 'undefined') {
                lucide.createIcons();
            }
            initializeNavigation();
            initializeTimeline();
            animateProgressCircle();
        });

        // Navigation
        function initializeNavigation() {
            const navItems = document.querySelectorAll('.nav-item');
            const sections = document.querySelectorAll('.content-section');
            
            navItems.forEach(item => {
                item.addEventListener('click', () => {
                    const targetSection = item.dataset.section;
                    showSection(targetSection);
                    
                    // Update active nav item
                    navItems.forEach(nav => nav.classList.remove('active'));
                    item.classList.add('active');
                });
            });
        }

        function showSection(sectionId) {
            const sections = document.querySelectorAll('.content-section');
            
            sections.forEach(section => {
                if (section.id === sectionId) {
                    section.classList.add('active');
                    section.style.display = 'block';
                } else {
                    section.classList.remove('active');
                    section.style.display = 'none';
                }
            });
            
            currentSection = sectionId;
            
            // Trigger section-specific animations
            if (sectionId === 'progress') {
                setTimeout(() => {
                    animateProgressCircle();
                    animateTimeline();
                }, 100);
            }
        }

        // Timeline
        function initializeTimeline() {
            const container = document.getElementById('timeline-items');
            if (!container) return;

            timelineData.forEach(item => {
                const timelineItem = document.createElement('div');
                timelineItem.className = 'timeline-item';
                
                timelineItem.innerHTML = `
                    <div class="timeline-dot ${item.status}"></div>
                    <div class="timeline-year">${item.year}</div>
                    <div class="timeline-event">${item.event}</div>
                `;
                
                container.appendChild(timelineItem);
            });
        }

        function animateTimeline() {
            const progressLine = document.getElementById('timeline-progress');
            if (progressLine) {
                setTimeout(() => {
                    progressLine.style.width = '62.5%'; // 5 out of 8 completed
                }, 500);
            }
        }

        // Progress Circle Animation
        function animateProgressCircle() {
            const numberElement = document.getElementById('progress-number');
            if (!numberElement) return;

            let current = 0;
            const target = 70; // 70% progress toward LEV
            const increment = target / 100;
            
            const timer = setInterval(() => {
                current += increment;
                if (current >= target) {
                    current = target;
                    clearInterval(timer);
                }
                numberElement.textContent = Math.round(current) + '%';
            }, 20);
        }

        // Keyboard navigation
        document.addEventListener('keydown', function(e) {
            const sections = ['progress', 'visionaries', 'protocols', 'research', 'resources', 'institute'];
            const currentIndex = sections.indexOf(currentSection);
            
            if (e.key === 'ArrowRight' && currentIndex < sections.length - 1) {
                const nextSection = sections[currentIndex + 1];
                document.querySelector(`[data-section="${nextSection}"]`).click();
            } else if (e.key === 'ArrowLeft' && currentIndex > 0) {
                const prevSection = sections[currentIndex - 1];
                document.querySelector(`[data-section="${prevSection}"]`).click();
            }
        });

        // Initialize animations on load
        setTimeout(() => {
            animateProgressCircle();
            animateTimeline();
        }, 1000);
    </script>
